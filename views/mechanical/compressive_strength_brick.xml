<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_compressive_strength_bricks_view" model="ir.ui.view">
        <field name="name">mechanical.compressive.strength.brick.form</field>
        <field name="model">mechanical.compressive.strength.brick</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" readonly="1"/>
                        <field name="parameter_id"/>
                       
                    </group>
                    <group>
                        <h1>
                            <field name="name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <field name="child_lines" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sr_no'/>
                                <field name='length'/>
                                <field name='width'/>
                                <field name='area'/>
                                <field name='load'/>
                                <field name='compressive_strength'/>
                               
                            </tree>
                           
                    </field> 
                           
                     
                </sheet>
            </form>
        </field>
    </record>
   


    </data>
</odoo>