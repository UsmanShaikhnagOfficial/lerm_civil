<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_splitting_tensile_strength_concrete_view" model="ir.ui.view">
        <field name="name">split.tensile.strength.concrete.form</field>
        <field name="model">split.tensile.strength.concrete</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" readonly="1"/>
                        <field name="parameter_id"/>
                       
                    </group>
                    <group>
                        <h1>
                            <field name="name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <field name="child_lines" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sr_no'/>
                                <field name='wt_of_cylinder'/>
                                <field name='heigth'/>
                                <field name='diameter'/>
                                <field name='breaking_load'/>
                                <field name='splite_tensile_strength'/>
                               
                            </tree>
                           
                    </field> 
                            <group>
                               <field name='average_splite_tensile_strength'/>
                            </group>
                     
                </sheet>
            </form>
        </field>
    </record>
  


    </data>
</odoo>