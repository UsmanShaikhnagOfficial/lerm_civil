<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_specific_gravity_water_absorption_view" model="ir.ui.view">
        <field name="name">specific.and.water.fine.aggregate.form</field>
        <field name="model">specific.and.water.fine.aggregate</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id"  readonly="1"/>
                        <field name="parameter_id"/>
                       
                    </group>
                    <group>
                        <h1>
                            <field name="name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <field name="child_lines" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sample_no'/>
                                <field name='wt_of_empty_pycnometer'/>
                                <field name='wt_of_pycnometer'/>
                                <field name='wt_of_pycnometer_surface_dry'/>
                                <field name='wt_of_pycnometer_surface_dry_water'/>
                                <field name='wt_of_saturated_surface_dry'/>
                                <field name='wt_of_oven_dried'/>
                                <field name="volume_of_water"/>
                                <field name="specific_gravity"/>
                                <field name="water_absorption"/>
                            </tree>
                           
                    </field> 
                          
                </sheet>
            </form>
        </field>
    </record>
   


    </data>
</odoo>