<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_miscellaneous_form" model="ir.ui.view">
            <field name="name">miscellaneouse.product.form</field>
            <field name="model">miscellaneouse.product</field>
            <field name="arch" type="xml">
                <form string="File Uploader">
                      <group >
                        <h1>
                            <field name="name_miscellaneous" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                     <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" />
                        <field name="eln_ref"/>
                        <field name="parameter_id" invisible="1"/>
                    </group>
                  <group>
                    <group>
                    <field name="report_file"/>
                    </group>
                    <group>
                    <field name="datasheet_file"/>
                    </group>

                    <group>
                    <field name="description"/>
                    </group>
                  </group>
                   <button
                            type="object"
                            class="oe_highlight" 
                            name="open_eln_page"
                            string="Submit"
                            />
                </form>
            </field>
        </record>
    </data>
</odoo>