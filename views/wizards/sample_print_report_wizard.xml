<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="print_reports_wizard" model="ir.ui.view">
        <field name="name">print.reports.wizard.form</field>
        <field name="model">print.reports.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                <group>
                    <field name="kes_no" />
                    
                </group>
                <footer>
                    <button
                            type="object"
                            class="oe_highlight" 
                            name="print_nabl_report"
                            string="Print NABL Report"/>
                    <button
                            type="object"
                            class="oe_highlight" 
                            name="print_non_nabl_report"
                            string="Print Non NABL Report"/>

                    <button
                            type="object"
                            class="oe_highlight" 
                            name="print_datasheet"
                            string="Print Datasheet"/>
                    <button
                            type="object"
                            name="discard_print"
                            string="Discard"/>
                </footer>
            </sheet>
            </form>
        </field>
        </record>
    </data>
</odoo>